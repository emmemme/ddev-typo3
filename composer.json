    "typo3/cms-core": "^10.2",
    "typo3/cms-dashboard": "^10.3",
    "typo3/cms-extbase": "^10.2",
    "typo3/cms-extensionmanager": "^10.2",
    "typo3/cms-felogin": "^10.2",
    "typo3/cms-filelist": "^10.2",
    "typo3/cms-filemetadata": "^10.2",
    "typo3/cms-fluid": "^10.2",
    "typo3/cms-fluid-styled-content": "^10.2",
    "typo3/cms-form": "^10.2",
    "typo3/cms-frontend": "^10.2",
    "typo3/cms-impexp": "^10.2",
    "typo3/cms-info": "^10.2",
    "typo3/cms-install": "^10.2",
