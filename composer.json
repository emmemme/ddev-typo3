    "typo3/cms-sys-note": "^10.3",
    "typo3/cms-t3editor": "^10.2",
    "typo3/cms-tstemplate": "^10.2",
    "typo3/cms-viewpage": "^10.2",
    "typo3/cms-workspaces": "^10.3"
  },
  "require-dev": {
    "typo3/cms-adminpanel": "^10.2",
    "helmich/typo3-typoscript-lint": "^2.1",
    "squizlabs/php_codesniffer": "^3.5"
